  "logo": "https://files.catbox.moe/qumhu4.jpg",
  "keywords": ["kaya", "whatsapp", "bot", "loader"],
  "env": {
    "SESSION_ID": {
      "description": "Votre session WhatsApp (doit commencer par kaya~)",
